{
  "name": "heimrichhannot/contao-migration-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 migration project bundle",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.19"
  },
  "suggest": {
    "heimrichhannot/contao-reader-bundle": "^1.0.0-beta45",
    "heimrichhannot/contao-list-bundle": "^1.0.0-beta46",
    "heimrichhannot/contao-filter-bundle": "^1.0.0-beta31",
    "heimrichhannot/contao-blocks": "^1.5.2"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\MigrationBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\MigrationBundle\\ContaoManager\\Plugin"
  }
}
